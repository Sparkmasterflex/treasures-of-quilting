o:Sass::Tree::RootNode
:@template"ò*@import "http://meyerweb.com/eric/tools/css/reset/reset.css";
@import 'mixins';

header, section, footer, aside, nav, article, figure, menu, time { display: block; }
menu { list-style: none; }

/*-------------------------Constants-*/
.hidden { text-indent: -9999px; overflow: hidden; }
.icon { display: block; width: 16px; height: 16px; }
.add { background: url(images/silk/image_add.png) no-repeat center center; }
.remove { background: url(images/silk/image_delete.png) no-repeat center center; }
.delete { background: url(images/silk/cross.png) no-repeat center center; }

.floatLeft { float: left; }
.floatRight { float: right; }

.dark-grad {
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#3B3B3B), to(#222222), color-stop(.3,#4F4F4F),color-stop(.8,#333333));
  background: -moz-linear-gradient(0% 78% 90deg,#222222, #3B3B3B, #4F4F4F 56%,#333333 93%);
}
.right-rounded { @include rounded-custom(0, 10px, 10px, 0); }
.left-rounded { @include rounded-custom(10px, 0, 0, 10px); }
.right-bottom-rounded { @include rounded-custom(0, 0, 10px, 0); }
.left-bottom-rounded { @include rounded-custom(0, 0, 0, 10px); }
.bottom-rounded { @include rounded-custom(0, 0, 10px, 10px); }

.button {
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#4D041B), to(#7A1333));
  background: -moz-linear-gradient(96% 75% 90deg,#7A1333, #4d041b);
  a {
    text: { align: center; shadow: -1px -1px 1px #555; decoration: none; }
    color: #fff;
    display: block;
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#E3AFAF), to(#4D041B), color-stop(.5,#821436),color-stop(.5,#6C112D));
    background: -moz-linear-gradient(0% 53% 90deg,#4D041B, #821436, #6C112D 0%,#E3AFAF 93%);
    &:hover {
      text-shadow: none;
      background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#A81A46), to(#A81A46), color-stop(.4,#4D041B),color-stop(.6,#4D041B));
      background: -moz-linear-gradient(0% 96% 90deg,#A81A46, #4D041B, #A81A46 100%);
    }
  }
}

.clearfix {
  display: inline-block;
  &:after {
    content: " ";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
  } }

a, p, h3 { color: #222; }
  p { text-shadow: 1px 1px 1px #ccc; }
  a { text-decoration: underline;
    &:hover { text-decoration: none; }
  }

a.next-prev { display: block; width: 24px; height: 12px;
  &:hover { background-position: top left;} }

html { height: 100%; }
body {
  font: { size: 62.5%; family: Tahoma, Arial, san-serif; }
  height: 100%;
  background: #dcdcdc;
}
footer, .inner {
  padding: 0 1em;
  width: 100.4em;
  margin: 0 auto;
}

li, a, p { font-size: 1.6em;}
p { line-height: 1.45em; margin-bottom: 0.63em;
  &.flash_msg {
    display: block;
    padding: 0.19em 0.625em;
    width: 100%;
    color: #000;
    &.notice { background: #EEE8AA; border: 1px solid #EEAD0E; }
    &.success {background: #D4ED91; border: 1px solid #385E0F; }
  }
}

/*----------------- Header ------------------*/
section#header{
  position: relative;
  padding: 1.5em 1.5em 0;
  width: 99.4em;
  margin: 0 auto;
  header {
    width: 99.4em;
    position: absolute;
    z-index: 2;
    height: 7em;
    overflow: visible;
    a { color: #fff; }
    #logo {
      background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#4D041B), to(#7A1333));
      background: -moz-linear-gradient(96% 75% 90deg,#7A1333, #4d041b);
      @include rounded(15px);
      padding: 0.5em;
      position: relative;
      h1 {
        background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#E3AFAF), to(#4D041B), color-stop(.5,#821436),color-stop(.5,#6C112D));
        background: -moz-linear-gradient(0% 53% 90deg,#4D041B, #821436, #6C112D 0%,#E3AFAF 93%);
        @include rounded(15px);
        a {
          display: block;
          width: 15em;
          height: 8.8em;
          background: url(images/logo_no_bg.png) no-repeat center center;
        }
      }
      h2 { height: 0; }
    } /*---- /#logo ----*/
    nav ul {
      float: right;
      padding: 0.5em;
      background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#4D041B), to(#7A1333));
      background: -moz-linear-gradient(96% 75% 90deg,#7A1333, #4d041b);
      @include rounded(15px);
      li { float: left;
        &.current a, a:hover {
          text-shadow: none;
          background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#A81A46), to(#A81A46), color-stop(.4,#4D041B),color-stop(.6,#4D041B));
          background: -moz-linear-gradient(0% 96% 90deg,#A81A46, #4D041B, #A81A46 100%);
        }
        a {
          text: { shadow: -1px -1px 1px #555; decoration: none; align: center;}
          font-size: 1.5em;
          display: block;
          width: 6em;
          padding: 0.9em 0;
          background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#E3AFAF), to(#4D041B), color-stop(.5,#821436),color-stop(.5,#6C112D));
          background: -moz-linear-gradient(0% 53% 90deg,#4D041B, #821436, #6C112D 0%,#E3AFAF 93%);
        }
      }
    } /*----- /nav ul -----*/
  } /*---- /header ----*/
} /*----- /#section#header -----*/

#content { width: 100%; margin-top: 7em; .inner { padding: 1.25em; } }

/*--------------------LeftNav--*/
#leftNav {
  background: #fff;
  @include rounded-custom(0, 0, 15px, 15px);
  width: 23em;
  padding: 2.5em 1em 1em;
  margin: 5.5em 0 1em .5em;
  float: left;
  h3 {
    margin-bottom: 0.25em;
    padding-bottom: 0.25em;
    border-bottom: 1px solid #222;
    a { @include left-nav; &:hover { background: url(images/rt_arrow.png) no-repeat center right; color: #4D041B; }}
    &.noLink { @include left-nav; }
  }
  menu { padding-left: 1em; margin-bottom: 0.7em;
    li { line-height: 1.5em; padding-left: 2em;
      &:hover { background: url(images/arrow.png) no-repeat center left; a { color: #4D041B; } }
      a {
        font: { size: 1em; weight: normal; }
        text-decoration: none;
        letter-spacing: .1em;        
        margin-left: -1em;
        text-indent: 1em;
      }
    }
  }
}

#mainContent {
  margin: 1em;
  width: 68.8em;
  padding: 2em 2em 0.5em;
  float: right;
  background: #fff;
  @include rounded(20px);
  @include shadow;
  &.minHeight { min-height: 42.5em; }
  h1 {
    font: { weight: normal; family: "Times New Roman", serif; size: 3em; }
    text-shadow: 1px 1px 1px #ccc;
    letter-spacing: 0.12em;
    margin-bottom: 0.33em;
  }
  h2 {
    text-shadow: 1px 1px 1px #ccc;
    font: { weight: normal; size: 1.8em; }
    margin-bottom: 0.56em;
    letter-spacing: 0.11em;
  }
  p { padding: 0 1.26em; }
  .leftcol { float: left; width: 34em; }
  .rightcol {
    float: right;
    width: 34em;
    position: relative;
    h1 { text-align: center; }
  }
  .dark-grad { padding: 1.7em 2.1em; }
  #learn {
    width: 29em;
    margin-right: 1em;
    float: right;
    position: absolute;
    top: 35em;
    a { color: #fff; padding: 0.25em 0; &:hover { color: #a81a46; }}
  }
}  /*---- /#mainContent ----*/

.leftcol {
  width: 34em;
  float: left;
  position: relative;
} /*---- /.leftcol ----*/

ul#slideshow {
  min-height: 35.2em;
  background: #000;
} /*---- /ul#slideshow ----*/
#controls {
  menu#slideLinks {
    width: 30em;
    position: absolute;
    bottom: 2.3em;
    a {
      &#previous, &#next { text-indent: -9999px; overflow: hidden; &:hover { background-position: bottom left;} }
      &#previous { float: left; background: url(images/image_arrow_prev.png) no-repeat top left; }
      &#next { float: right; background: url(images/image_arrow_next.png) no-repeat top left; }
      &#learnMore { font-size: 1em; }
    }
  }
  p {
    margin-bottom: 0;
    padding: 0 2em;
    color: #fff;
    text-shadow: 1px 1px 1px #444;
  }
}

/*---- js_gallery ----*/
ul#gallery {
  li {
    float: left;
    width: 5em;
    height: 5em;
    background: #000;
    border: 2px solid #7A1333;
    margin: 0.31em 0.75em;
  }
}


#featured {
  width:98.4em;
  padding: 1em;
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#4D041B), to(#7A1333));
  background: -moz-linear-gradient(96% 75% 90deg,#7A1333, #4d041b);
  @include rounded(20px);
  h4 {
    font: { size: 2.4em; weight: normal; }
    text-align: center;
    letter-spacing: 0.26em;
    margin-bottom: 0.42em;
  }
  .middle {
    margin: 0 1em;
    padding: 1em 2em;
    width: 28em;
  }
  .feature {
    padding: 1em;
    width: 30em;
    height: 20em;
    float: left;
    background: #fff;
    a { font-size: 2em; padding: 0.60em 0; width: 8.5em; }
  }
}

.thumb-80 {
  border: 4px solid #222;
  float: left;
  overflow: hidden;
  margin: 0 0.5em;
  height: 8em;
}

.button-link {
  margin: 2em auto;
  width: 17em;
  padding: 0.35em;
}


/*-------------------Pagination-----*/
a#subpages-link{
  float: left;
  display: block;
  width: 25%;
}

.pagination {
  margin-right: 2em;
  width: 72%;
  text-align: right;
  span, em { font-size: 1.4em; }
}

/*---------------------------------Sales Template---*/
#subpage-preview {
  position: relative;
  margin-top: 2em;
  clear: both;
  .section ul { margin: 1em 0; }
  #sub-nav { width: 100%;
    a#subpages-link { position: relative; z-index: 150; }
    .drop-down {
      background: -moz-linear-gradient(top, #7A1333 44%, #4D041B 98%);
      background: -webkit-gradient(linear, left top, left bottom, color-stop(44%,#7A1333), color-stop(98%,#4D041B));
      filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#7A1333', endColorstr='#4D041B',GradientType=0 );
      -webkit-border-radius: 5px;
      -moz-border-radius: 5px;
      border: 2px solid #3A0306;
      padding: 3.7em 1em 2em;
      position: absolute;
      top: -1em;
      left: -1em;
      z-index: 130;
      h3 { font-size: 1.6em; border-bottom: 1px solid #fff; color: #fff; }
      a { color: #fff; text-shadow: 1px 1px 1px #555; font-size: 0.875em; }
      .webpage { width: 20em; margin-right: 1em; ul li { margin: 0.31em 0;} }
    }
  }
  menu#sub-page-desc {
    width: 100%;
    margin: 1em 0;
    padding: 1em 0;
    border-top: 1px solid #ccc;
    li {
      cursor: pointer;
      width: 8.7em;
      min-height: 12.5em;
      padding: 0.63em;
      margin: 0 0.63em 0 0;
      float: left;
      -webkit-border-radius: 5px;
      -moz-border-radius: 5px;
      border: 1px solid #ccc;
      text-align: center;
      &:hover {
        background: #ddd;
        border: 1px solid #999;
      }
      h2 { font-size: 1.2em; height: 2.2em; vertical-align: center; }
      p, a { font-size: 0.8em; text-align: left; }
      p { padding: 0; height: 9.71em; }
    }
  }
}

/*--------------------------------Footer-*/
footer {
  padding: 1em;
  width: 100.4em;
  height: 3em;
  margin: 0 auto;
  nav ul {
    width: 60em;
    margin: 0 auto;
    padding-bottom: 1em;
    li { float: left; margin: 0 1.25em;
      &.current a { color: #4D041B; text-decoration: underline; }
      a {
        color: #222;
        font-size: 1.5em;
        text-decoration: none;
        &:hover {color: #4D041B; text-decoration: underline;}
      }
    }
  } /*---- /nav ul ----*/
